{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["off"],
    "no-else-return": ["off"],
    "import/prefer-default-export": ["off"],
    "import/extensions": ["error", "always"]
  }
}
